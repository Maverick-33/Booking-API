{
  "name": "express-bookings",
  "version": "1.0.0",
  "type": "module",
  "description": "Your REST API for managing bookings",
  "main": "src/index.js",
  "scripts": {
    "seed": "prisma db seed",
    "dev": "nodemon src/index.js",
    "test-positive": "newman run \"./postman/collections/Bookings API.json\" -e \"./postman/environments/Local.postman_environment.json\"",
    "test-negative": "newman run \"./postman/collections/Bookings API Negative.json\" -e \"./postman/environments/Local.postman_environment.json\"",
    "test": "npm run test-positive && npm run test-negative && npm run sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org na-zmu --project node-express tsconfig.json && sentry-cli sourcemaps upload --org na-zmu --project node-express tsconfig.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@sentry/cli": "^2.39.1",
    "@sentry/node": "^7.120.0",
    "@sentry/profiling-node": "^8.42.0",
    "@sentry/tracing": "^7.114.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "newman": "^6.0.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18 <19"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/index.js",
      "args": [],
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
